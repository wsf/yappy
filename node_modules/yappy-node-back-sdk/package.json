{
  "name": "yappy-node-back-sdk",
  "version": "1.1.222",
  "description": "SDK de Yappy para Node",
  "main": "dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js && npm run build:replace_interfaces_file && npm pack",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:replace_interfaces_file": "cp ./src/types/common/main.d.ts ./dist/types/common/ && rm ./dist/types/common/main.d.js",
    "test": "jest",
    "minify": "minify dist -d dist2"
  },
  "author": "Banco General de Panam√°",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "process": "^0.11.10"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.10",
    "babel-minify": "^0.5.1",
    "babel-preset-minify": "^0.5.1",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "node-mocks-http": "^1.10.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^7.0.1",
    "typescript": "^4.2.3"
  }
}
